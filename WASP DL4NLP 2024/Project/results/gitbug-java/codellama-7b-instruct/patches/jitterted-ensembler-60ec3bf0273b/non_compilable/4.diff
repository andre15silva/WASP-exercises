+public class Ensemble {
+    private Set<Integer> membersWhoAccepted = new HashSet<>();
+    private Set<Integer> membersAsSpectators = new HashSet<>();
+
+    public void accept(Integer memberId) {
+        membersWhoAccepted.add(memberId);
+    }
+
+    public void decline(Integer memberId) {
+        membersWhoAccepted.remove(memberId);
+    }
+
+    public void joinAsSpectator(Integer memberId) {
