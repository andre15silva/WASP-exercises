+public static class FormElement extends Element {
+    final private static Parser parser = new Parser();
+
+    FormElement(String tagName, String baseUri, String baseUri2) {
+        super(tagName, baseUri, baseUri2);
+    }
+
+    public List<Connection.KeyVal> formData() {
+        List<Connection.KeyVal> formData = new ArrayList<Connection.KeyVal>();
+        for (Node formChild : childNodes()) {
+            if (formChild instanceof FormElement) {
+                continue; // don't include nested forms
+            }
+            String key = null;
+            String val = null;
+            if (formChild instanceof Element) {
+                Element el = (Element) formChild;
+                if (el.tagName().equals("input")) {
+                    key = el.attr("name");
+                    val = el.attr("value");
+                } else if (el.tagName().equals("select")) {
+                    key = el.attr("name");
+                    val = el.val();
+                }
+            }
+            if (key != null && key.length() > 0) {
+                formData.add(Connection.KeyVal.create(key, val));
+            }
+        }
+        return formData;
+    }
+}
