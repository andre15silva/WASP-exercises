+public MultiValueMap classToMap(Class mapClass) {
+    Descriptor descriptor = Descriptors.getDescriptor(mapClass);
+    MultiValueMap mvmap = new MultiValueMap();
+    for (Entry field : descriptor.getFields()) {
+        mvmap.add(field.getName(), field.getType());
+    }
+    return mvmap;
+}
